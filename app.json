{
  "expo": {
    "name": "taskify-mobile",
    "slug": "taskify-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elham5.taskify-mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.elham5.taskifymobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "launcher"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "13861138-d88b-497c-be77-7a4fe757eaf6"
      }
    },
    "scheme": "mycoolredirect"
  }
}
